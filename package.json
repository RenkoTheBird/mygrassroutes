    "server": "node server.js",
    "start": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev:frontend\"",
    "ngrok:frontend": "ngrok http 5173",
    "ngrok:backend": "ngrok http 3001",
    "ngrok:all": "ngrok start --all --config ngrok.yml"
  },
